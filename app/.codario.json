{
    "integrations": {
        "git_provider": {
            "credential": "github-general",
            "target": "stage/updates/26"
        }
    },
    "update_behavior": {
        "auto_start_updates": "auto_for_vulnerable",
        "skip_manual_testing": "no",
        "source_branch": "stage/updates/26",
        "destination_branch": "updates/[task:id]",
        "handle_packages": "dev_and_prod",
        "create_tasks_for": "contains_vulnerable_packages"
    },
    "events": {
        "test_passed": [
            {
                "accept_pull_request": {
                    "delete_source_branch": "yes"
                }
            }
        ],
        "test_failed": [
            {
                "decline_pull_request": {
                    "delete_source_branch": "no"
                }
            }
        ]
    }
}